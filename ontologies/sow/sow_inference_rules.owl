<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns="https://agentic-data-scraper.com/ontology/sow#"
         xml:base="https://agentic-data-scraper.com/ontology/sow"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:swrl="http://www.w3.org/2003/11/swrl#"
         xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
         xmlns:dcterms="http://purl.org/dc/terms/"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">

  <owl:Ontology rdf:about="https://agentic-data-scraper.com/ontology/sow">
    <owl:versionIRI rdf:resource="https://agentic-data-scraper.com/ontology/sow/v1.0"/>
    <owl:versionInfo>1.0.0</owl:versionInfo>
    <owl:imports rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:label>SOW Inference Rules Ontology</rdfs:label>
    <rdfs:comment>OWL ontology defining intelligent inference rules for Semantic Statement of Work processing, connected to Gist upper ontology via DBC bridge</rdfs:comment>
    <dcterms:title>SOW Inference Rules Ontology</dcterms:title>
    <dcterms:description>Intelligent inference rules for automated discovery of analytical opportunities from business requirements in Statement of Work documents</dcterms:description>
    <dcterms:creator>Agentic Data Scraper Project</dcterms:creator>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-09-24</dcterms:created>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-09-24</dcterms:modified>
    <dcterms:license rdf:resource="https://agentic-data-scraper.com/license"/>
  </owl:Ontology>

  <!-- ====================================================================== -->
  <!-- SKOS CONCEPT SCHEMES FOR TAXONOMIC BROWSING                           -->
  <!-- ====================================================================== -->

  <!-- AI Inference Taxonomy ConceptScheme -->
  <skos:ConceptScheme rdf:about="#InferenceTaxonomy">
    <rdfs:label>AI Inference Taxonomy</rdfs:label>
    <rdfs:comment>Classification of AI-inferred opportunities and domain entities</rdfs:comment>
    <dcterms:description>Hierarchical organization of business requirements, inference rules, and automatically discovered analytical opportunities across spatial, temporal, network, and cross-domain analysis patterns</dcterms:description>
    <rdfs:seeAlso rdf:resource="#BusinessRequirement"/>
    <rdfs:seeAlso rdf:resource="#InferredOpportunity"/>
    <rdfs:seeAlso rdf:resource="#SpatialAnalysisOpportunity"/>
    <rdfs:seeAlso rdf:resource="#TemporalAnalysisOpportunity"/>
    <rdfs:seeAlso rdf:resource="#NetworkAnalysisOpportunity"/>
    <rdfs:seeAlso rdf:resource="#CrossDomainOpportunity"/>
  </skos:ConceptScheme>

  <!-- Core Classes -->
  <owl:Class rdf:about="#BusinessRequirement">
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#InferenceTaxonomy"/>
    <rdfs:label>Business Requirement</rdfs:label>
    <rdfs:comment>A requirement explicitly stated by business stakeholders in a Statement of Work document, representing the direct needs and constraints of the business that must be satisfied by the data pipeline solution</rdfs:comment>
    <skos:scopeNote>Represents explicitly stated requirements from business stakeholders that form the foundation for automated analytical opportunity inference. These requirements must be parsed, analyzed, and semantically enriched to trigger appropriate SWRL inference rules. Lambda implementations must support: (1) Natural language processing pipelines with named entity recognition, relationship extraction, and intent classification to structure unstructured requirement text, (2) Requirement validation frameworks ensuring completeness, consistency, and feasibility with automated gap analysis and stakeholder feedback loops, (3) Requirement traceability systems linking generated capabilities back to source requirements for impact analysis and change management, (4) Semantic annotation engines that tag requirements with domain ontology concepts to enable inference rule activation, (5) Requirement versioning and evolution tracking supporting iterative refinement and change impact analysis. Architecture should implement requirements-driven code generation where each business requirement maps to specific data pipeline components, quality checks, and monitoring capabilities with full bidirectional traceability.</skos:scopeNote>
    <skos:example>"Monitor supplier performance for all olive oil vendors in Turkey and Greece with weekly quality assessments and cost tracking" - directly stated requirement from procurement team</skos:example>
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/sow"/>
    <rdfs:seeAlso rdf:resource="#InferredOpportunity"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/complete-sow#BusinessChallenge"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/complete-sow#DesiredOutcome"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge#DataBusinessCanvas"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/Demand"/>
    <rdfs:seeAlso rdf:resource="http://purl.org/dc/terms/requires"/>
    <rdfs:seeAlso rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </owl:Class>

  <owl:Class rdf:about="#InferredOpportunity">
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#InferenceTaxonomy"/>
    <!-- Hierarchical SKOS relationships for taxonomic browsing -->
    <skos:narrower rdf:resource="#SpatialAnalysisOpportunity"/>
    <skos:narrower rdf:resource="#TemporalAnalysisOpportunity"/>
    <skos:narrower rdf:resource="#NetworkAnalysisOpportunity"/>
    <skos:narrower rdf:resource="#CrossDomainOpportunity"/>
    <rdfs:label>Inferred Opportunity</rdfs:label>
    <rdfs:comment>An analytical opportunity automatically inferred from business requirements using SWRL reasoning rules, representing unstated but valuable analysis possibilities that align with business goals</rdfs:comment>
    <skos:scopeNote>Represents analytical capabilities that emerge from semantic reasoning over explicit business requirements, revealing valuable but unstated analysis opportunities that significantly enhance business value. These opportunities are discovered through SWRL rule execution and domain expertise encoded in inference patterns. Lambda implementations must support: (1) Inference confidence scoring and validation mechanisms with domain expert review workflows and business impact assessment, (2) Automated code generation for inferred analytical capabilities with appropriate data processing patterns, algorithms, and performance optimization, (3) Business value quantification models that estimate ROI and strategic impact of implementing inferred opportunities versus explicit requirements only, (4) Inference explanation systems providing transparent reasoning traces that business stakeholders can validate and approve, (5) Iterative inference refinement with machine learning to improve inference accuracy based on stakeholder feedback and business outcome measurement. Consider implementing active learning systems where successful inferences improve rule confidence scores and failed inferences trigger rule refinement or human expert consultation.</skos:scopeNote>
    <skos:example>From requirement "track supplier costs" system infers opportunity for "temporal cost trend analysis with seasonal pattern detection" - unstated analytics that add business value</skos:example>
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/sow"/>
    <rdfs:seeAlso rdf:resource="#BusinessRequirement"/>
    <rdfs:seeAlso rdf:resource="#SpatialAnalysisOpportunity"/>
    <rdfs:seeAlso rdf:resource="#TemporalAnalysisOpportunity"/>
    <rdfs:seeAlso rdf:resource="#NetworkAnalysisOpportunity"/>
    <rdfs:seeAlso rdf:resource="#CrossDomainOpportunity"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/complete-sow#AnalyticalOpportunity"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge#IntelligenceCapability"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge#ValueProposition"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/Opportunity"/>
    <rdfs:seeAlso rdf:resource="http://purl.org/dc/terms/type"/>
  </owl:Class>

  <owl:Class rdf:about="#SpatialAnalysisOpportunity">
    <rdfs:subClassOf rdf:resource="#InferredOpportunity"/>
    <skos:inScheme rdf:resource="#InferenceTaxonomy"/>
    <!-- Hierarchical SKOS relationships for taxonomic browsing -->
    <skos:broader rdf:resource="#InferredOpportunity"/>
    <skos:related rdf:resource="#SupplyChainEntity"/>
    <rdfs:label>Spatial Analysis Opportunity</rdfs:label>
    <rdfs:comment>An inferred analytical opportunity involving geographic or location-based analysis, such as regional risk assessment, geographic cost variations, or spatial distribution patterns</rdfs:comment>
    <skos:scopeNote>Encompasses location-aware analytics that reveal geographic patterns, spatial relationships, and regional insights critical for supply chain optimization, risk assessment, and market expansion strategies. Spatial analysis often uncovers non-obvious relationships between geography and business performance. Lambda implementations require: (1) Geospatial data processing capabilities with coordinate system transformations, spatial indexing (R-tree, Quadtree), and geographic information system (GIS) integration for map-based visualizations, (2) Spatial analysis algorithms including clustering (DBSCAN), hotspot analysis (Getis-Ord Gi*), spatial interpolation (kriging), and geographic weighted regression for location-dependent modeling, (3) Multi-resolution spatial analysis from building-level to continental-scale with automated scale selection based on data density and business context, (4) Real-time location tracking and geofencing with mobile device integration, GPS/RFID data streams, and location-based event triggering, (5) Spatial optimization algorithms for facility location, route optimization, territory design, and service area planning with constraint satisfaction programming. Consider implementing spatial-temporal analysis for dynamic location patterns and predictive spatial modeling for expansion planning and risk assessment.</skos:scopeNote>
    <skos:example>Geographic clustering analysis of Turkish olive oil suppliers to identify optimal logistics hubs and regional quality patterns based on Aegean vs Mediterranean coastal production zones</skos:example>
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/sow"/>
    <rdfs:seeAlso rdf:resource="#SupplyChainEntity"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/complete-sow#SpatialContext"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/GeoCoordinates"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/Place"/>
    <rdfs:seeAlso rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
  </owl:Class>

  <owl:Class rdf:about="#TemporalAnalysisOpportunity">
    <rdfs:subClassOf rdf:resource="#InferredOpportunity"/>
    <skos:inScheme rdf:resource="#InferenceTaxonomy"/>
    <!-- Hierarchical SKOS relationships for taxonomic browsing -->
    <skos:broader rdf:resource="#InferredOpportunity"/>
    <skos:related rdf:resource="#FinancialEntity"/>
    <rdfs:label>Temporal Analysis Opportunity</rdfs:label>
    <rdfs:comment>An inferred analytical opportunity involving time-based analysis, such as trend detection, seasonality patterns, historical comparisons, or temporal forecasting</rdfs:comment>
    <skos:scopeNote>Encompasses time-series analysis, trend detection, seasonality modeling, and predictive forecasting that reveal temporal patterns essential for strategic planning, resource allocation, and risk management. Temporal analysis transforms historical data into forward-looking strategic intelligence. Lambda implementations require: (1) Time-series data processing pipelines with automatic frequency detection, missing data imputation, and outlier detection using statistical methods (IQR, Z-score) and domain-specific rules, (2) Advanced forecasting algorithms including ARIMA, Prophet, LSTM neural networks, and ensemble methods with automated model selection based on cross-validation performance and business context, (3) Seasonality decomposition and pattern recognition with automatic seasonal adjustment, holiday effect modeling, and external factor integration (weather, economic indicators, geopolitical events), (4) Real-time anomaly detection with dynamic baseline adjustment and contextual alerting based on business calendars and operational patterns, (5) Multi-horizon forecasting with uncertainty quantification providing confidence intervals and scenario analysis for strategic decision-making. Consider implementing temporal feature engineering for machine learning models and temporal data fusion combining multiple time-series sources for comprehensive temporal intelligence.</skos:scopeNote>
    <skos:example>Seasonal trend analysis of olive oil prices and harvest yields to predict optimal procurement timing with 3-month forecasting based on Mediterranean climate patterns</skos:example>
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/sow"/>
    <rdfs:seeAlso rdf:resource="#FinancialEntity"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/complete-sow#TemporalContext"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/DateTime"/>
    <rdfs:seeAlso rdf:resource="http://purl.org/dc/terms/temporal"/>
    <rdfs:seeAlso rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
  </owl:Class>

  <owl:Class rdf:about="#NetworkAnalysisOpportunity">
    <rdfs:subClassOf rdf:resource="#InferredOpportunity"/>
    <skos:inScheme rdf:resource="#InferenceTaxonomy"/>
    <!-- Hierarchical SKOS relationships for taxonomic browsing -->
    <skos:broader rdf:resource="#InferredOpportunity"/>
    <rdfs:label>Network Analysis Opportunity</rdfs:label>
    <rdfs:comment>An inferred analytical opportunity involving relationship or connectivity analysis, such as regulatory networks, compliance dependencies, or organizational relationship mapping</rdfs:comment>
    <skos:scopeNote>Encompasses graph-based analytics including social networks, supply chain networks, regulatory compliance networks, and organizational relationship structures. These analyses reveal emergent properties, critical paths, influence patterns, and systemic risks that are invisible in traditional tabular analysis. Lambda implementations require: (1) Graph database technologies (Neo4j, Amazon Neptune) optimized for traversal algorithms and pattern matching queries, (2) Scalable graph processing frameworks supporting distributed computation for large-scale network analysis (Apache Spark GraphX, Graph Neural Networks), (3) Network visualization and interactive exploration tools with force-directed layouts, hierarchical clustering, and community detection algorithms, (4) Real-time graph streaming for dynamic network evolution tracking with temporal network analysis capabilities, (5) Graph machine learning pipelines for node classification, link prediction, and graph embedding generation. Consider implementing graph-based feature engineering for traditional ML models, network-aware anomaly detection for fraud/compliance monitoring, and centrality-based influence ranking for stakeholder management and risk assessment.</skos:scopeNote>
    <skos:example>EU organic certification compliance network analysis mapping regulatory dependencies between Turkish suppliers, certification bodies, and European import requirements</skos:example>
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/sow"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/complete-sow#RegulatoryConstraint"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/Organization"/>
    <rdfs:seeAlso rdf:resource="http://xmlns.com/foaf/0.1/knows"/>
    <rdfs:seeAlso rdf:resource="http://purl.org/dc/terms/relation"/>
  </owl:Class>

  <owl:Class rdf:about="#CrossDomainOpportunity">
    <rdfs:subClassOf rdf:resource="#InferredOpportunity"/>
    <skos:inScheme rdf:resource="#InferenceTaxonomy"/>
    <!-- Hierarchical SKOS relationships for taxonomic browsing -->
    <skos:broader rdf:resource="#InferredOpportunity"/>
    <skos:related rdf:resource="#SupplyChainEntity"/>
    <skos:related rdf:resource="#FinancialEntity"/>
    <skos:related rdf:resource="#CommodityEntity"/>
    <rdfs:label>Cross Domain Opportunity</rdfs:label>
    <rdfs:comment>An inferred analytical opportunity spanning multiple business domains, such as combining supply chain and financial analysis, or integrating commodity trends with geopolitical risk assessment</rdfs:comment>
    <skos:scopeNote>Represents sophisticated analytical opportunities that integrate insights across traditionally separate business domains, requiring synthesis of heterogeneous data sources, domain expertise, and analytical methodologies. These opportunities typically deliver exponentially higher business value through systemic insights that single-domain analysis cannot reveal. Lambda implementations must architect for: (1) Domain-agnostic data integration with semantic harmonization across different ontologies, data models, and business vocabularies using knowledge graphs and entity resolution, (2) Multi-modal analytics pipelines combining structured data (databases), unstructured data (documents, social media), and external signals (market data, weather, geopolitical events), (3) Federated machine learning enabling model training across domains without data sharing, preserving privacy and security boundaries while capturing cross-domain patterns, (4) Causal inference and correlation analysis with domain expertise integration to distinguish spurious correlations from actionable insights, (5) Explainable AI frameworks that can articulate cross-domain dependencies in business-friendly terms for executive decision-making. Consider implementing digital twin architectures for complex system modeling and scenario planning across multiple business domains.</skos:scopeNote>
    <skos:example>Integrated analysis combining Turkish supplier logistics costs with olive oil commodity price fluctuations and EUR/TRY exchange rate risks for comprehensive procurement planning</skos:example>
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/sow"/>
    <rdfs:seeAlso rdf:resource="#SupplyChainEntity"/>
    <rdfs:seeAlso rdf:resource="#FinancialEntity"/>
    <rdfs:seeAlso rdf:resource="#CommodityEntity"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/complete-sow#CrossDomainOpportunity"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/complete-sow#DomainContext"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge#IntelligenceCapability"/>
    <rdfs:seeAlso rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
  </owl:Class>

  <!-- Domain-Specific Classes -->
  <owl:Class rdf:about="#SupplyChainEntity">
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#InferenceTaxonomy"/>
    <!-- Hierarchical SKOS relationships for taxonomic browsing -->
    <skos:related rdf:resource="#FinancialEntity"/>
    <skos:related rdf:resource="#CommodityEntity"/>
    <rdfs:label>Supply Chain Entity</rdfs:label>
    <rdfs:comment>Any entity mentioned in a supply chain context within business requirements, including suppliers, warehouses, logistics providers, distribution centers, or inventory management systems</rdfs:comment>
    <skos:scopeNote>Represents physical and logical entities within supply chain networks that require tracking, optimization, and integration across multiple dimensions (spatial, temporal, financial, operational). Supply chain entities are inherently complex with multiple identities, relationships, and data sources that must be unified for comprehensive analysis. Lambda implementations require: (1) Entity resolution and master data management to unify supplier identities across systems (ERP, procurement, logistics, financial) with fuzzy matching and duplicate detection, (2) Multi-tiered supplier relationship mapping including direct suppliers, sub-suppliers, and alternative suppliers with risk assessment and qualification status tracking, (3) Real-time supply chain visibility with IoT integration for shipment tracking, warehouse operations, and inventory levels across the network, (4) Supply chain risk modeling including geographic concentration risk, single-source dependencies, financial stability assessment, and regulatory compliance status, (5) Dynamic supplier performance scoring with multi-dimensional KPIs including quality, delivery, cost, sustainability, and innovation metrics. Consider implementing digital supply chain twin architectures with event-driven simulation capabilities for scenario planning and risk mitigation strategy evaluation.</skos:scopeNote>
    <skos:example>"Anatolia Olive Cooperative" supplier entity with warehouse locations in Izmir and Ayvalik, logistics partner DHL Express, and inventory management system tracking 500MT monthly capacity</skos:example>
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/sow"/>
    <rdfs:seeAlso rdf:resource="#SpatialAnalysisOpportunity"/>
    <rdfs:seeAlso rdf:resource="#CrossDomainOpportunity"/>
    <rdfs:seeAlso rdf:resource="#FinancialEntity"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/Organization"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/Place"/>
    <rdfs:seeAlso rdf:resource="http://purl.org/goodrelations/v1#BusinessEntity"/>
  </owl:Class>

  <owl:Class rdf:about="#FinancialEntity">
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#InferenceTaxonomy"/>
    <!-- Hierarchical SKOS relationships for taxonomic browsing -->
    <skos:related rdf:resource="#SupplyChainEntity"/>
    <skos:related rdf:resource="#CommodityEntity"/>
    <rdfs:label>Financial Entity</rdfs:label>
    <rdfs:comment>Any entity mentioned in a financial context within business requirements, including cost centers, revenue streams, budget items, financial metrics, or accounting systems</rdfs:comment>
    <skos:scopeNote>Represents financial constructs including cost centers, revenue streams, budget allocations, financial instruments, and accounting entities that require integration with operational data for comprehensive business performance analysis. Financial entities are subject to complex regulatory requirements, multi-currency considerations, and time-sensitive calculations requiring high precision and auditability. Lambda implementations must support: (1) Financial data integration across multiple systems (ERP, budgeting, treasury, tax) with reconciliation capabilities and audit trail preservation, (2) Multi-currency processing with real-time exchange rates, historical rate tracking, and currency risk assessment for global operations, (3) Financial performance attribution analysis linking operational metrics to financial outcomes with cost allocation models and profitability analysis, (4) Regulatory compliance automation for financial reporting (IFRS, GAAP) with automated journal entry generation and variance analysis, (5) Financial forecasting and scenario modeling with sensitivity analysis and Monte Carlo simulation for risk assessment. Consider implementing financial data mesh architectures with domain-specific financial data products, automated financial close processes, and real-time financial performance monitoring with exception-based alerting.</skos:scopeNote>
    <skos:example>"Mediterranean Procurement Budget 2025" cost center with €2.5M allocation tracked through SAP system with KPIs for cost-per-ton and currency hedging performance</skos:example>
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/sow"/>
    <rdfs:seeAlso rdf:resource="#TemporalAnalysisOpportunity"/>
    <rdfs:seeAlso rdf:resource="#CrossDomainOpportunity"/>
    <rdfs:seeAlso rdf:resource="#SupplyChainEntity"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge#CostEvent"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge#RevenueEvent"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/MonetaryAmount"/>
    <rdfs:seeAlso rdf:resource="http://purl.org/goodrelations/v1#PriceSpecification"/>
  </owl:Class>

  <owl:Class rdf:about="#CommodityEntity">
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#InferenceTaxonomy"/>
    <!-- Hierarchical SKOS relationships for taxonomic browsing -->
    <skos:related rdf:resource="#SupplyChainEntity"/>
    <skos:related rdf:resource="#FinancialEntity"/>
    <rdfs:label>Commodity Entity</rdfs:label>
    <rdfs:comment>Any entity mentioned in a commodities context within business requirements, including raw materials, energy resources, agricultural products, precious metals, or commodity trading markets</rdfs:comment>
    <skos:scopeNote>Represents tradeable goods and raw materials that are subject to global market forces, price volatility, and complex supply-demand dynamics influenced by geopolitical events, weather patterns, and economic cycles. Commodity entities require sophisticated analysis integrating market data, supply chain intelligence, and external factors. Lambda implementations must support: (1) Real-time commodity market data integration with major exchanges (CBOT, ICE, LME) including futures pricing, volatility calculations, and basis differentials for location-specific pricing, (2) Commodity price forecasting models incorporating supply-demand fundamentals, technical analysis, and external factor correlation (weather, geopolitical risk, currency fluctuations), (3) Supply chain risk assessment for commodity sourcing including producer concentration analysis, geographic diversification evaluation, and alternative sourcing scenario modeling, (4) Quality and grading systems integration with automated quality assessment, certification tracking, and premium/discount calculations based on quality specifications, (5) Hedging strategy optimization with financial instrument evaluation (futures, options, swaps) and risk management portfolio analysis. Consider implementing commodity digital twins that model the entire value chain from production through consumption with scenario planning capabilities for strategic procurement and risk management decisions.</skos:scopeNote>
    <skos:example>"Extra Virgin Olive Oil Grade A" commodity tracked through London International Financial Futures Exchange (LIFFE) with benchmarks tied to Mediterranean harvest yields and quality certifications</skos:example>
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/sow"/>
    <rdfs:seeAlso rdf:resource="#CrossDomainOpportunity"/>
    <rdfs:seeAlso rdf:resource="#SupplyChainEntity"/>
    <rdfs:seeAlso rdf:resource="#FinancialEntity"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/Product"/>
    <rdfs:seeAlso rdf:resource="http://purl.org/goodrelations/v1#ProductOrService"/>
    <rdfs:seeAlso rdf:resource="http://purl.org/dc/terms/subject"/>
  </owl:Class>

  <!-- Properties -->
  <owl:ObjectProperty rdf:about="#mentionsEntity">
    <rdfs:label>mentions entity</rdfs:label>
    <rdfs:comment>Indicates that a business requirement explicitly mentions or references a specific entity (supply chain, financial, or commodity entity) that may trigger cross-domain inference rules</rdfs:comment>
    <rdfs:domain rdf:resource="#BusinessRequirement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/sow"/>
    <skos:example>ProcurementRequirement mentionsEntity AnatoliaOliveCooperative - business requirement explicitly references a supply chain entity triggering spatial analysis rules</skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#isMentionedBy">
    <owl:inverseOf rdf:resource="#mentionsEntity"/>
    <rdfs:label>is mentioned by</rdfs:label>
    <rdfs:comment>Inverse of mentionsEntity - indicates that an entity is mentioned by a business requirement</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:range rdf:resource="#BusinessRequirement"/>
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/sow"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#infersOpportunity">
    <rdfs:label>infers opportunity</rdfs:label>
    <rdfs:comment>Relates a business requirement to an analytical opportunity that was automatically inferred through SWRL reasoning rules, establishing the provenance of inferred knowledge</rdfs:comment>
    <rdfs:domain rdf:resource="#BusinessRequirement"/>
    <rdfs:range rdf:resource="#InferredOpportunity"/>
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/sow"/>
    <skos:example>CostTrackingRequirement infersOpportunity TemporalTrendAnalysis - requirement to track costs automatically triggers temporal analysis capability generation</skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#isOpportunityInferredFrom">
    <owl:inverseOf rdf:resource="#infersOpportunity"/>
    <rdfs:label>is opportunity inferred from</rdfs:label>
    <rdfs:comment>Inverse of infersOpportunity - relates an inferred opportunity back to the business requirement it was inferred from</rdfs:comment>
    <rdfs:domain rdf:resource="#InferredOpportunity"/>
    <rdfs:range rdf:resource="#BusinessRequirement"/>
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/sow"/>
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="#confidenceLevel">
    <rdfs:label>confidence level</rdfs:label>
    <rdfs:comment>A decimal value between 0.0 and 1.0 indicating the confidence level of an inferred opportunity, based on the strength of the reasoning pattern and domain expertise</rdfs:comment>
    <rdfs:domain rdf:resource="#InferredOpportunity"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/sow"/>
    <skos:example>GeographicRiskAnalysis confidenceLevel "0.87" - high confidence that supplier mentions warrant spatial analysis based on domain expertise and rule strength</skos:example>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#reasoningTrace">
    <rdfs:label>reasoning trace</rdfs:label>
    <rdfs:comment>A human-readable explanation of the reasoning process that led to the inference of this opportunity, providing transparency and auditability for automated decision-making</rdfs:comment>
    <rdfs:domain rdf:resource="#InferredOpportunity"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
