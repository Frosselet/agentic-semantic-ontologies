  <!-- Example Task -->
  <bridge:DataProcessingTask rdf:about="#TurkishSupplierDataCollection">
    <rdfs:label>Turkish Supplier Data Collection Task</rdfs:label>
    <bridge:createsBusinessValue rdf:resource="#SupplierTransparencyValue"/>
  </bridge:DataProcessingTask>

  <!-- Example Executive Target -->
  <bridge:ExecutiveTarget rdf:about="#CostReductionTarget">
    <rdfs:label>15% Supply Chain Cost Reduction</rdfs:label>
    <bridge:ownedBy rdf:resource="#SupplyChainDirector"/>
  </bridge:ExecutiveTarget>

</rdf:RDF>