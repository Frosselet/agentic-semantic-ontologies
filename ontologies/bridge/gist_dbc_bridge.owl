  </bridge:SemanticSOWContract>

  <!-- Example Data Contract -->
  <bridge:DataContract rdf:about="#OliveOilDataContract">
    <rdfs:label>Olive Oil Supply Data Contract</rdfs:label>
    <bridge:executedByTask rdf:resource="#TurkishSupplierDataCollection"/>
