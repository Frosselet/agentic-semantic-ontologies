  <!-- Channel to Customer Segment and Value Proposition -->
  <owl:ObjectProperty rdf:about="#deliversValueThrough">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:domain rdf:resource="#ValueProposition"/>
    <rdfs:range rdf:resource="#Channel"/>
    <rdfs:label>delivers value through</rdfs:label>
    <rdfs:comment>Links value proposition to delivery channels</rdfs:comment>
    <skos:example>PredictiveSupplyChainAnalytics deliversValueThrough SelfServiceAPIPortal</skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#reachesCustomerVia">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:domain rdf:resource="#Channel"/>
