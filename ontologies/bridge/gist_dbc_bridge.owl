<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns="https://agentic-data-scraper.com/ontology/gist-dbc-bridge#"
         xml:base="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:dcterms="http://purl.org/dc/terms/"
         xmlns:gist="https://w3id.org/semanticarts/ontology/gistCore#"
         xmlns:dbc="http://data-business-canvas.org/ontology#"
         xmlns:sow="https://agentic-data-scraper.com/ontology/sow#"
         xmlns:bmc="http://business-model-canvas.org/ontology#"
         xmlns:bridge="https://agentic-data-scraper.com/ontology/gist-dbc-bridge#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">

  <owl:Ontology rdf:about="https://agentic-data-scraper.com/ontology/gist-dbc-bridge">
    <owl:versionIRI rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge/v1.0"/>
    <owl:versionInfo>1.0.0</owl:versionInfo>
    <owl:imports rdf:resource="https://w3id.org/semanticarts/ontology/gistCore"/>
    <owl:imports rdf:resource="http://data-business-canvas.org/ontology"/>
    <owl:imports rdf:resource="https://agentic-data-scraper.com/ontology/sow"/>
    <rdfs:label>Gist to Data Business Canvas Bridge Ontology</rdfs:label>
    <rdfs:comment>Bridge ontology connecting Semantic Arts Gist upper ontology to Data Business Canvas, SOW, and Data Contracts</rdfs:comment>
    <dcterms:created>2024-12-01T00:00:00Z</dcterms:created>
    <dcterms:modified>2024-12-24T00:00:00Z</dcterms:modified>
    <dcterms:creator>Agentic Data Scraper Team</dcterms:creator>
    <dcterms:contributor>Claude Code - Semantic Architecture</dcterms:contributor>
    <dcterms:description>This bridge ontology provides semantic integration between the Gist upper ontology, Data Business Canvas, Statement of Work (SOW), and Data Contract layers, enabling Lambda-ready data pipeline generation with full semantic discoverability.</dcterms:description>
    <dcterms:license>MIT</dcterms:license>
  </owl:Ontology>

  <!-- ====================================================================== -->
  <!-- SKOS CONCEPT SCHEMES FOR TAXONOMIC BROWSING                           -->
  <!-- ====================================================================== -->

  <!-- Data Business Canvas Taxonomy ConceptScheme -->
  <skos:ConceptScheme rdf:about="#DataBusinessTaxonomy">
    <rdfs:label>Data Business Canvas Taxonomy</rdfs:label>
    <rdfs:comment>Hierarchical classification of data business model concepts following complete Business Model Canvas structure</rdfs:comment>
    <dcterms:description>Enables browsable navigation of business model components from strategic to operational levels, covering all 9 BMC building blocks adapted for data businesses</dcterms:description>
    <rdfs:seeAlso rdf:resource="#DataBusinessCanvas"/>
    <!-- Customer-facing components -->
    <rdfs:seeAlso rdf:resource="#CustomerSegment"/>
    <rdfs:seeAlso rdf:resource="#ValueProposition"/>
    <rdfs:seeAlso rdf:resource="#Channel"/>
    <rdfs:seeAlso rdf:resource="#CustomerRelationship"/>
    <rdfs:seeAlso rdf:resource="#RevenueStream"/>
    <!-- Internal components -->
    <rdfs:seeAlso rdf:resource="#KeyResource"/>
    <rdfs:seeAlso rdf:resource="#KeyActivity"/>
    <rdfs:seeAlso rdf:resource="#KeyPartner"/>
    <rdfs:seeAlso rdf:resource="#CostStructure"/>
    <!-- Data-specific extensions -->
    <rdfs:seeAlso rdf:resource="#DataAsset"/>
    <rdfs:seeAlso rdf:resource="#IntelligenceCapability"/>
    <rdfs:seeAlso rdf:resource="#DataProduct"/>
    <rdfs:seeAlso rdf:resource="#DataService"/>
    <rdfs:seeAlso rdf:resource="#DataGovernanceRole"/>
    <rdfs:seeAlso rdf:resource="#DataMonetizationPattern"/>
    <rdfs:seeAlso rdf:resource="#DataLifecycleStage"/>
    <rdfs:seeAlso rdf:resource="#DataQualityDimension"/>
  </skos:ConceptScheme>

  <!-- Executive Strategy Taxonomy ConceptScheme -->
  <skos:ConceptScheme rdf:about="#ExecutiveTaxonomy">
    <rdfs:label>Executive Strategy Taxonomy</rdfs:label>
    <rdfs:comment>Strategic executive concepts and alignment structures</rdfs:comment>
    <dcterms:description>Hierarchical classification of executive targets, strategic alignment, and organizational objectives for strategic decision-making support</dcterms:description>
    <rdfs:seeAlso rdf:resource="#ExecutiveTarget"/>
    <rdfs:seeAlso rdf:resource="#ExecutiveTargetOwner"/>
    <rdfs:seeAlso rdf:resource="#StrategicAlignmentScore"/>
  </skos:ConceptScheme>

  <!-- ====================================================================== -->
  <!-- LEVEL 1: Gist Foundation Classes (Upper Ontology)                     -->
  <!-- ====================================================================== -->

  <!-- Key Gist classes we extend (these come from gist import) -->
  <!-- gist:Organization, gist:Person, gist:Event, gist:Agreement, gist:Category -->

  <!-- ====================================================================== -->
  <!-- LEVEL 2: Data Business Canvas Classes (Business Strategy Layer)       -->
  <!-- ====================================================================== -->

  <!-- Data Business Canvas Core - Extends Gist Organization -->
  <owl:Class rdf:about="#DataBusinessCanvas">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Organization"/>
    <rdfs:subClassOf rdf:resource="http://business-model-canvas.org/ontology#BusinessModel"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <!-- Hierarchical SKOS relationships for taxonomic browsing - Complete BMC structure -->
    <!-- Customer-facing building blocks -->
    <skos:narrower rdf:resource="#CustomerSegment"/>
    <skos:narrower rdf:resource="#ValueProposition"/>
    <skos:narrower rdf:resource="#Channel"/>
    <skos:narrower rdf:resource="#CustomerRelationship"/>
    <skos:narrower rdf:resource="#RevenueStream"/>
    <!-- Internal building blocks -->
    <skos:narrower rdf:resource="#KeyResource"/>
    <skos:narrower rdf:resource="#KeyActivity"/>
    <skos:narrower rdf:resource="#KeyPartner"/>
    <skos:narrower rdf:resource="#CostStructure"/>
    <!-- Data-specific extensions (already subtypes of KeyResource/KeyActivity) -->
    <skos:narrower rdf:resource="#DataAsset"/>
    <skos:narrower rdf:resource="#IntelligenceCapability"/>
    <!-- Strategic integration -->
    <skos:related rdf:resource="#ExecutiveTarget"/>
    <skos:related rdf:resource="#SemanticSOWContract"/>
    <rdfs:label>Data Business Canvas</rdfs:label>
    <rdfs:comment>A business model specifically focused on data-driven value creation, extending Gist Organization with complete BMC structure</rdfs:comment>
    <skos:scopeNote>This concept extends traditional Business Model Canvas methodology to emphasize data assets and intelligence capabilities as core business components. Unlike generic business models that treat data as supporting infrastructure, this model positions data assets as primary value drivers and revenue generators. Lambda implementations must consider: (1) Data lineage tracking from ingestion to business value creation, (2) Multi-tenant data governance with role-based access controls, (3) Real-time data quality monitoring with automated alerts, (4) Cross-functional data product lifecycle management, (5) Semantic data discovery through ontology-based cataloging. Implementation requires event-driven architecture supporting both batch and streaming data processing, with data contracts enforcing schema evolution and backward compatibility. Consider serverless data mesh patterns for scalable domain-driven data architecture.</skos:scopeNote>
    <skos:example>Supply chain visibility platform for Turkish olive oil producers with predictive analytics for seasonal demand forecasting and real-time supplier performance dashboards</skos:example>
    <rdfs:seeAlso rdf:resource="#ValueProposition"/>
    <rdfs:seeAlso rdf:resource="#CustomerSegment"/>
    <rdfs:seeAlso rdf:resource="#DataAsset"/>
    <rdfs:seeAlso rdf:resource="#ExecutiveTarget"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/complete-sow#SemanticStatementOfWork"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/Organization"/>
    <rdfs:seeAlso rdf:resource="http://purl.org/dc/terms/subject"/>
    <rdfs:seeAlso rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
  </owl:Class>

  <!-- Business Model Components as Gist Categories -->
  <owl:Class rdf:about="#ValueProposition">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <!-- Hierarchical SKOS relationships for taxonomic browsing -->
    <skos:broader rdf:resource="#DataBusinessCanvas"/>
    <skos:related rdf:resource="#CustomerSegment"/>
    <skos:related rdf:resource="#DataAsset"/>
    <skos:related rdf:resource="#IntelligenceCapability"/>
    <rdfs:label>Value Proposition</rdfs:label>
    <rdfs:comment>Data-driven value proposition extending Gist Category</rdfs:comment>
    <skos:example>Reduce procurement costs by 15% through real-time supplier performance analytics and geographical risk assessment of Mediterranean olive oil supply chains</skos:example>
    <rdfs:seeAlso rdf:resource="#CustomerSegment"/>
    <rdfs:seeAlso rdf:resource="#DataAsset"/>
    <rdfs:seeAlso rdf:resource="#IntelligenceCapability"/>
    <rdfs:seeAlso rdf:resource="#RevenueEvent"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/complete-sow#DesiredOutcome"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/Product"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/Service"/>
    <rdfs:seeAlso rdf:resource="http://purl.org/goodrelations/v1#ProductOrService"/>
  </owl:Class>

  <owl:Class rdf:about="#CustomerSegment">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <!-- Hierarchical SKOS relationships for taxonomic browsing -->
    <skos:broader rdf:resource="#DataBusinessCanvas"/>
    <skos:related rdf:resource="#ValueProposition"/>
    <skos:related rdf:resource="#DataAsset"/>
    <skos:related rdf:resource="#IntelligenceCapability"/>
    <rdfs:label>Customer Segment</rdfs:label>
    <rdfs:comment>Customer segment classification extending Gist Category</rdfs:comment>
    <skos:example>Mid-size Mediterranean food manufacturers (50-500 employees) requiring supply chain transparency for premium olive oil procurement and regulatory compliance</skos:example>
    <rdfs:seeAlso rdf:resource="#ValueProposition"/>
    <rdfs:seeAlso rdf:resource="#RevenueEvent"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/audience"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/Person"/>
    <rdfs:seeAlso rdf:resource="http://xmlns.com/foaf/0.1/Group"/>
    <rdfs:seeAlso rdf:resource="http://purl.org/dc/terms/audience"/>
  </owl:Class>

  <owl:Class rdf:about="#DataAsset">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <rdfs:subClassOf rdf:resource="http://data-business-canvas.org/ontology#KeyResource"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <!-- Hierarchical SKOS relationships for taxonomic browsing -->
    <skos:broader rdf:resource="#DataBusinessCanvas"/>
    <skos:related rdf:resource="#ValueProposition"/>
    <skos:related rdf:resource="#CustomerSegment"/>
    <skos:related rdf:resource="#IntelligenceCapability"/>
    <rdfs:label>Data Asset</rdfs:label>
    <rdfs:comment>Business-valuable data resource extending Gist Category and DBC Key Resource</rdfs:comment>
    <skos:scopeNote>Represents data resources that have been curated, governed, and productized for business consumption, distinct from raw data sources. Data Assets must maintain business context, semantic meaning, and operational metadata throughout their lifecycle. Lambda implementations require: (1) Data asset registry with semantic annotations, business glossary integration, and impact analysis capabilities, (2) Multi-tiered storage architecture optimizing for access patterns (hot/warm/cold with automated lifecycle policies), (3) Data quality monitoring with configurable rules, anomaly detection, and business impact assessment, (4) Comprehensive data lineage tracking from source systems through transformations to consumption endpoints, (5) Access control integration with corporate identity systems and fine-grained permission management. Consider implementing data mesh principles with domain-owned data products, standardized data contracts for interoperability, and federated governance balancing autonomy with enterprise consistency. Storage formats should optimize for both analytical queries (columnar formats like Parquet/ORC) and operational access patterns (key-value, document, graph as appropriate).</skos:scopeNote>
    <skos:example>Turkish olive oil supplier database with GPS coordinates, harvest yield history, quality certifications, and real-time delivery performance metrics stored in Parquet format</skos:example>
    <rdfs:seeAlso rdf:resource="#IntelligenceCapability"/>
    <rdfs:seeAlso rdf:resource="#DataQualityStandard"/>
    <rdfs:seeAlso rdf:resource="#DataProcessingTask"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/complete-sow#EntityToTrack"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/sow#SupplyChainEntity"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/Dataset"/>
    <rdfs:seeAlso rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
    <rdfs:seeAlso rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
    <rdfs:seeAlso rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
  </owl:Class>

  <owl:Class rdf:about="#IntelligenceCapability">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <rdfs:subClassOf rdf:resource="http://data-business-canvas.org/ontology#KeyActivity"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <!-- Hierarchical SKOS relationships for taxonomic browsing -->
    <skos:broader rdf:resource="#DataBusinessCanvas"/>
    <skos:related rdf:resource="#ValueProposition"/>
    <skos:related rdf:resource="#CustomerSegment"/>
    <skos:related rdf:resource="#DataAsset"/>
    <rdfs:label>Intelligence Capability</rdfs:label>
    <rdfs:comment>AI/ML capability that transforms data assets into business value</rdfs:comment>
    <skos:scopeNote>Represents AI/ML capabilities that transform raw data assets into actionable business intelligence and automated decision-making systems. This encompasses not just algorithmic components but the entire operational infrastructure including model governance, performance monitoring, and feedback loops. Lambda implementations should architect for: (1) MLOps pipeline automation with model versioning using semantic versioning tied to data contract changes, (2) Multi-environment model deployment with canary releases and A/B testing frameworks, (3) Model drift detection using statistical tests and business KPI monitoring, (4) Explainable AI capabilities for regulatory compliance and business transparency, (5) Real-time inference engines with sub-second response times and 99.9% availability. Consider containerized model serving with auto-scaling based on prediction volume, feature stores for consistent feature engineering across batch/streaming contexts, and model lineage tracking linking predictions back to training data provenance.</skos:scopeNote>
    <skos:example>Predictive analytics engine using time-series forecasting to predict olive oil harvest yields based on weather data, historical performance, and soil quality indicators</skos:example>
    <rdfs:seeAlso rdf:resource="#DataAsset"/>
    <rdfs:seeAlso rdf:resource="#DataProcessingTask"/>
    <rdfs:seeAlso rdf:resource="#ValueProposition"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/sow#InferredOpportunity"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/sow#CrossDomainOpportunity"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/SoftwareApplication"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/ComputerLanguage"/>
    <rdfs:seeAlso rdf:resource="http://purl.org/dc/terms/type"/>
  </owl:Class>

  <!-- ====================================================================== -->
  <!-- DATA-SPECIFIC BUSINESS CONCEPTS (DBC Extensions)                       -->
  <!-- ====================================================================== -->

  <!-- Data Product - Packaged data offering -->
  <owl:Class rdf:about="#DataProduct">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="#ValueProposition"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <skos:broader rdf:resource="#ValueProposition"/>
    <skos:related rdf:resource="#DataAsset"/>
    <skos:related rdf:resource="#DataService"/>
    <rdfs:label>Data Product</rdfs:label>
    <rdfs:comment>Packaged, productized data offering with defined schema, SLA, and business value</rdfs:comment>
    <skos:scopeNote>Data products are self-contained, discoverable, and addressable data assets with clear ownership, documented schemas, quality guarantees, and versioning. Unlike raw data assets, products have explicit consumer contracts, usage analytics, and lifecycle management. They implement data mesh principles with domain ownership and federated governance.</skos:scopeNote>
    <skos:example>Supplier Risk Score API providing daily-updated risk ratings for 10,000+ Mediterranean food suppliers with 99.9% uptime SLA and JSON schema v2.1</skos:example>
  </owl:Class>

  <!-- Data Service - Ongoing data delivery mechanism -->
  <owl:Class rdf:about="#DataService">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="#Channel"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <skos:broader rdf:resource="#Channel"/>
    <skos:related rdf:resource="#DataProduct"/>
    <skos:related rdf:resource="#DataAsset"/>
    <rdfs:label>Data Service</rdfs:label>
    <rdfs:comment>Operational service delivering data products to consumers via API, streaming, or batch mechanisms</rdfs:comment>
    <skos:scopeNote>Data services provide continuous access to data products with defined interfaces (REST API, GraphQL, event streams, batch exports), authentication/authorization, rate limiting, monitoring, and support. Services abstract technical complexity and provide consistent access patterns across heterogeneous data sources.</skos:scopeNote>
    <skos:example>Real-time supplier inventory WebSocket feed pushing updates every 5 minutes with OAuth2 authentication and 1000 req/hour rate limit</skos:example>
  </owl:Class>

  <!-- Data Governance Role - Responsibility in data management -->
  <owl:Class rdf:about="#DataGovernanceRole">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <skos:broader rdf:resource="#KeyActivity"/>
    <skos:related rdf:resource="#DataAsset"/>
    <skos:related rdf:resource="#DataQualityStandard"/>
    <rdfs:label>Data Governance Role</rdfs:label>
    <rdfs:comment>Defined responsibility for data stewardship, quality, security, and compliance</rdfs:comment>
    <skos:scopeNote>Governance roles establish accountability for data assets across their lifecycle including data owners (business accountability), data stewards (operational management), data custodians (technical implementation), and data consumers (appropriate usage). Roles define decision rights for access, quality standards, retention, and business glossary management.</skos:scopeNote>
    <skos:example>Supply Chain Data Steward role responsible for supplier master data quality, quarterly audits, and business glossary maintenance for 50+ data elements</skos:example>
  </owl:Class>

  <!-- Data Monetization Pattern - Revenue generation approach -->
  <owl:Class rdf:about="#DataMonetizationPattern">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="#RevenueStream"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <skos:broader rdf:resource="#RevenueStream"/>
    <skos:related rdf:resource="#DataProduct"/>
    <skos:related rdf:resource="#ValueProposition"/>
    <rdfs:label>Data Monetization Pattern</rdfs:label>
    <rdfs:comment>Strategic approach to generating revenue from data assets and intelligence capabilities</rdfs:comment>
    <skos:scopeNote>Monetization patterns include: (1) Direct data sales (licensing datasets or API access), (2) Data-enhanced products (embedding insights into existing offerings), (3) Data-as-a-Service subscriptions (recurring access fees), (4) Marketplace/brokerage (facilitating data exchange for commission), (5) Insights-as-a-Service (selling analytics not raw data), (6) Advertising/targeting (using data to improve ad relevance). Pattern selection depends on data uniqueness, customer willingness-to-pay, and competitive landscape.</skos:scopeNote>
    <skos:example>Tiered subscription model: Basic API access ($500/month, 10K requests), Professional ($2000/month, 100K requests, priority support), Enterprise (custom pricing, unlimited requests, dedicated account manager)</skos:example>
  </owl:Class>

  <!-- Data Lifecycle Stage - Maturity phase of data asset -->
  <owl:Class rdf:about="#DataLifecycleStage">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <skos:related rdf:resource="#DataAsset"/>
    <skos:related rdf:resource="#DataProduct"/>
    <rdfs:label>Data Lifecycle Stage</rdfs:label>
    <rdfs:comment>Phase in data asset evolution from acquisition through archival or deletion</rdfs:comment>
    <skos:scopeNote>Lifecycle stages include: (1) Acquisition (sourcing and ingestion), (2) Curation (cleansing, enrichment, standardization), (3) Storage (optimization for access patterns), (4) Processing (transformation and analysis), (5) Distribution (delivery to consumers), (6) Archival (long-term retention), (7) Deletion (secure disposal). Each stage has distinct cost structures, SLAs, and governance requirements.</skos:scopeNote>
    <skos:example>Supplier master data lifecycle: Daily acquisition via API → Real-time validation and enrichment → Hot storage in PostgreSQL → Nightly aggregation for analytics → Quarterly archival to S3 Glacier → 7-year retention before GDPR-compliant deletion</skos:example>
  </owl:Class>

  <!-- Data Quality Dimension - Aspect of data quality measurement -->
  <owl:Class rdf:about="#DataQualityDimension">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="#DataQualityStandard"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <skos:broader rdf:resource="#DataQualityStandard"/>
    <skos:related rdf:resource="#DataAsset"/>
    <rdfs:label>Data Quality Dimension</rdfs:label>
    <rdfs:comment>Measurable aspect of data quality with defined metrics and thresholds</rdfs:comment>
    <skos:scopeNote>Standard quality dimensions include: (1) Accuracy (correctness vs. reality), (2) Completeness (presence of required values), (3) Consistency (agreement across systems), (4) Timeliness (freshness for business needs), (5) Validity (conformance to schema/rules), (6) Uniqueness (absence of duplicates). Each dimension requires specific measurement techniques and remediation workflows.</skos:scopeNote>
    <skos:example>Supplier address accuracy dimension: 95% threshold measured by geocoding validation, monthly spot checks against official registries, automated remediation workflows for discrepancies</skos:example>
  </owl:Class>

  <!-- Channels - How value reaches customers -->
  <owl:Class rdf:about="#Channel">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <skos:broader rdf:resource="#DataBusinessCanvas"/>
    <skos:related rdf:resource="#CustomerSegment"/>
    <skos:related rdf:resource="#ValueProposition"/>
    <rdfs:label>Channel</rdfs:label>
    <rdfs:comment>Distribution and communication channel through which value propositions reach customer segments</rdfs:comment>
    <skos:scopeNote>Represents the pathways through which data products, insights, and intelligence capabilities are delivered to customers. In data business models, channels encompass both technical delivery mechanisms (APIs, data feeds, dashboards, embedded analytics) and business delivery models (self-service portals, managed services, white-label solutions). Lambda implementations require: (1) Multi-channel API gateway with rate limiting, authentication, and usage analytics per customer segment, (2) Channel-specific data transformation pipelines adapting to delivery format requirements (REST, GraphQL, Webhook, Batch file drops), (3) Channel performance monitoring tracking delivery SLAs, latency, throughput, and customer engagement metrics, (4) Channel cost attribution for profitability analysis by delivery mechanism, (5) A/B testing frameworks for channel optimization and customer preference learning. Architecture should support omnichannel strategies where customers can consume data through multiple synchronized channels with consistent experience.</skos:scopeNote>
    <skos:example>Self-service API portal providing real-time supplier performance data, weekly email reports with predictive analytics insights, and embedded Power BI dashboards for procurement teams</skos:example>
    <rdfs:seeAlso rdf:resource="#CustomerSegment"/>
    <rdfs:seeAlso rdf:resource="#ValueProposition"/>
    <rdfs:seeAlso rdf:resource="#CustomerRelationship"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/DeliveryMethod"/>
  </owl:Class>

  <!-- Customer Relationships - How to interact with segments -->
  <owl:Class rdf:about="#CustomerRelationship">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <skos:broader rdf:resource="#DataBusinessCanvas"/>
    <skos:related rdf:resource="#CustomerSegment"/>
    <skos:related rdf:resource="#Channel"/>
    <rdfs:label>Customer Relationship</rdfs:label>
    <rdfs:comment>Type of relationship established and maintained with specific customer segments</rdfs:comment>
    <skos:scopeNote>Defines the engagement model and support structure for different customer segments consuming data products. Data businesses require sophisticated relationship strategies balancing automation with high-touch support based on customer value and complexity. Lambda implementations should support: (1) Tiered customer success programs with automated onboarding, training resources, and escalation paths based on customer segment and contract value, (2) Usage analytics and adoption tracking identifying at-risk customers and expansion opportunities, (3) Proactive data quality alerts and issue resolution with SLA-based response times, (4) Customer feedback loops integrated into product roadmap planning, (5) Community platforms for peer-to-peer knowledge sharing and use case discovery. Consider implementing product-led growth motions for self-service segments and white-glove concierge services for enterprise accounts.</skos:scopeNote>
    <skos:example>Automated onboarding with interactive tutorials for SMB customers, dedicated account management with quarterly business reviews for enterprise clients, and community forum with expert office hours for developer ecosystem</skos:example>
    <rdfs:seeAlso rdf:resource="#CustomerSegment"/>
    <rdfs:seeAlso rdf:resource="#Channel"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/CustomerService"/>
  </owl:Class>

  <!-- Key Activities - Core business activities -->
  <owl:Class rdf:about="#KeyActivity">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <rdfs:subClassOf rdf:resource="http://data-business-canvas.org/ontology#KeyActivity"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <skos:broader rdf:resource="#DataBusinessCanvas"/>
    <skos:narrower rdf:resource="#IntelligenceCapability"/>
    <skos:related rdf:resource="#ValueProposition"/>
    <skos:related rdf:resource="#KeyResource"/>
    <rdfs:label>Key Activity</rdfs:label>
    <rdfs:comment>Most important activities required to make the data business model work</rdfs:comment>
    <skos:scopeNote>Encompasses the critical operational activities necessary to create, deliver, and capture value from data. Beyond technical data processing, this includes activities like data sourcing and acquisition, quality assurance and governance, product development and innovation, customer support and success, and platform operations and maintenance. Lambda implementations must architect for: (1) Activity-based cost accounting tracking resource consumption and ROI by activity type, (2) Process automation identifying high-volume repetitive activities suitable for ML-driven automation, (3) Activity dependency mapping for critical path analysis and bottleneck identification, (4) Capacity planning and resource allocation optimizing activity throughput, (5) Continuous improvement frameworks measuring and optimizing activity efficiency over time. IntelligenceCapability is a specific subtype focusing on AI/ML transformation activities.</skos:scopeNote>
    <skos:example>Daily data acquisition from 500+ supplier websites, real-time data quality validation and anomaly detection, weekly model retraining for demand forecasting, monthly customer success check-ins, and continuous platform security monitoring</skos:example>
    <rdfs:seeAlso rdf:resource="#IntelligenceCapability"/>
    <rdfs:seeAlso rdf:resource="#ValueProposition"/>
    <rdfs:seeAlso rdf:resource="#DataProcessingTask"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/Action"/>
  </owl:Class>

  <!-- Key Resources - Strategic assets beyond data -->
  <owl:Class rdf:about="#KeyResource">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <rdfs:subClassOf rdf:resource="http://data-business-canvas.org/ontology#KeyResource"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <skos:broader rdf:resource="#DataBusinessCanvas"/>
    <skos:narrower rdf:resource="#DataAsset"/>
    <skos:related rdf:resource="#KeyActivity"/>
    <skos:related rdf:resource="#ValueProposition"/>
    <rdfs:label>Key Resource</rdfs:label>
    <rdfs:comment>Most important strategic assets required to make the data business model work</rdfs:comment>
    <skos:scopeNote>Represents critical assets beyond data including intellectual property (algorithms, models, domain expertise), technical infrastructure (cloud platforms, data pipelines, ML frameworks), human capital (data scientists, domain experts, customer success teams), brand and reputation, and strategic partnerships. Lambda implementations should track: (1) Resource utilization and ROI analysis across all asset categories, (2) Resource capacity planning with forecasting for growth scenarios, (3) Build-vs-buy analysis for strategic resource acquisition decisions, (4) Resource risk assessment and redundancy planning for business continuity, (5) Asset lifecycle management including depreciation, maintenance, and refresh cycles. DataAsset is a specific subtype focusing on data resources.</skos:scopeNote>
    <skos:example>Proprietary demand forecasting algorithms (IP), AWS infrastructure with multi-region deployment (technical), team of 15 data scientists with supply chain domain expertise (human), established brand in Mediterranean agriculture vertical (intangible)</skos:example>
    <rdfs:seeAlso rdf:resource="#DataAsset"/>
    <rdfs:seeAlso rdf:resource="#KeyActivity"/>
    <rdfs:seeAlso rdf:resource="#KeyPartner"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/Product"/>
  </owl:Class>

  <!-- Revenue Streams - How value is monetized -->
  <owl:Class rdf:about="#RevenueStream">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <skos:broader rdf:resource="#DataBusinessCanvas"/>
    <skos:related rdf:resource="#ValueProposition"/>
    <skos:related rdf:resource="#CustomerSegment"/>
    <skos:related rdf:resource="#RevenueEvent"/>
    <rdfs:label>Revenue Stream</rdfs:label>
    <rdfs:comment>Ways the business generates revenue from customer segments</rdfs:comment>
    <skos:scopeNote>Defines the monetization model and pricing strategy for data products and services. Data businesses typically employ hybrid revenue models combining subscription (recurring revenue for ongoing data access), usage-based (pay-per-query or API call), outcome-based (performance fees tied to business results), licensing (data asset or model licensing fees), and professional services (implementation and customization fees). Lambda implementations require: (1) Multi-model billing engine supporting complex pricing rules, tiered plans, usage metering, and overage charges, (2) Revenue recognition automation complying with accounting standards (ASC 606/IFRS 15), (3) Customer lifetime value (CLV) analytics and churn prediction models, (4) A/B testing frameworks for pricing optimization, (5) Contract and subscription management with automated renewals and expansion tracking. Architecture should support real-time usage tracking for accurate billing and enable flexible pricing experimentation.</skos:scopeNote>
    <skos:example>Monthly subscription at €1,500/month for up to 10,000 API calls, usage-based pricing at €0.05 per additional API call, annual licensing fee of €50,000 for proprietary forecasting models, and professional services at €2,500/day for custom integration</skos:example>
    <rdfs:seeAlso rdf:resource="#ValueProposition"/>
    <rdfs:seeAlso rdf:resource="#CustomerSegment"/>
    <rdfs:seeAlso rdf:resource="#RevenueEvent"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/PriceSpecification"/>
  </owl:Class>

  <!-- Cost Structure - Business costs and expenses -->
  <owl:Class rdf:about="#CostStructure">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Category"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <skos:broader rdf:resource="#DataBusinessCanvas"/>
    <skos:related rdf:resource="#KeyActivity"/>
    <skos:related rdf:resource="#KeyResource"/>
    <skos:related rdf:resource="#CostEvent"/>
    <rdfs:label>Cost Structure</rdfs:label>
    <rdfs:comment>All costs and expenses incurred to operate the data business model</rdfs:comment>
    <skos:scopeNote>Encompasses the full cost profile of operating a data business including infrastructure costs (cloud compute, storage, networking), data acquisition costs (licensing, partnerships, scraping infrastructure), personnel costs (engineering, data science, operations, sales), technology costs (SaaS tools, ML platforms, security), and customer acquisition costs. Data businesses typically exhibit high fixed costs (platform development, infrastructure baseline) with relatively low marginal costs per additional customer, enabling strong unit economics at scale. Lambda implementations should provide: (1) Activity-based costing allocating shared infrastructure costs to specific business activities and customer segments, (2) Cost optimization recommendations using FinOps principles for cloud resource efficiency, (3) Marginal cost analysis for pricing decisions and profitability modeling, (4) Budget forecasting with scenario planning for different growth trajectories, (5) Cost anomaly detection identifying unexpected spend increases. Architecture should support multi-dimensional cost allocation and real-time cost visibility.</skos:scopeNote>
    <skos:example>AWS infrastructure €15,000/month (30%), data scientist salaries €45,000/month (45%), third-party data licenses €8,000/month (16%), and SaaS tools €4,500/month (9%)</skos:example>
    <rdfs:seeAlso rdf:resource="#KeyActivity"/>
    <rdfs:seeAlso rdf:resource="#KeyResource"/>
    <rdfs:seeAlso rdf:resource="#CostEvent"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/MonetaryAmount"/>
  </owl:Class>

  <!-- Key Partners as Gist Organizations -->
  <owl:Class rdf:about="#KeyPartner">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Organization"/>
    <skos:inScheme rdf:resource="#DataBusinessTaxonomy"/>
    <skos:broader rdf:resource="#DataBusinessCanvas"/>
    <skos:narrower rdf:resource="#DataProvider"/>
    <skos:narrower rdf:resource="#TechnologyPartner"/>
    <skos:related rdf:resource="#KeyActivity"/>
    <skos:related rdf:resource="#KeyResource"/>
    <rdfs:label>Key Partner</rdfs:label>
    <rdfs:comment>Network of strategic partners and suppliers that make the business model work</rdfs:comment>
    <skos:scopeNote>Represents strategic external organizations critical to business model execution including data providers (supplying source data), technology partners (providing infrastructure or specialized capabilities), distribution partners (accessing new customer segments), and ecosystem partners (complementary services enhancing value proposition). Partner strategies vary from transactional vendor relationships to deep strategic alliances with joint value creation. Lambda implementations should support: (1) Partner relationship management tracking performance, SLAs, and contract terms, (2) Partner contribution attribution measuring business value delivered by each partnership, (3) Partner risk assessment monitoring dependencies and developing contingency plans, (4) Partner collaboration platforms enabling data sharing, joint product development, and co-selling motions, (5) Partner ecosystem analytics identifying gaps and expansion opportunities. DataProvider and TechnologyPartner are specialized subtypes.</skos:scopeNote>
    <skos:example>Strategic partnership with Turkish Agricultural Ministry for authoritative supplier certification data, technology alliance with Snowflake for data warehousing infrastructure, distribution partnership with SAP for embedded procurement analytics</skos:example>
    <rdfs:seeAlso rdf:resource="#DataProvider"/>
    <rdfs:seeAlso rdf:resource="#TechnologyPartner"/>
    <rdfs:seeAlso rdf:resource="#KeyResource"/>
    <rdfs:seeAlso rdf:resource="https://schema.org/Organization"/>
  </owl:Class>

  <!-- Data Provider as specialized Key Partner -->
  <owl:Class rdf:about="#DataProvider">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Organization"/>
    <rdfs:label>Data Provider</rdfs:label>
    <rdfs:comment>External organization providing data services</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#TechnologyPartner">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Organization"/>
    <rdfs:label>Technology Partner</rdfs:label>
    <rdfs:comment>Organization providing technology infrastructure or services</rdfs:comment>
  </owl:Class>

  <!-- Revenue and Cost Structures as Gist Events -->
  <owl:Class rdf:about="#RevenueEvent">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Event"/>
    <rdfs:label>Revenue Event</rdfs:label>
    <rdfs:comment>Event that generates revenue from data insights</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#CostEvent">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Event"/>
    <rdfs:label>Cost Event</rdfs:label>
    <rdfs:comment>Event that incurs costs in data operations</rdfs:comment>
  </owl:Class>

  <!-- ====================================================================== -->
  <!-- LEVEL 3: SOW (Statement of Work) Classes (Implementation Layer)       -->
  <!-- ====================================================================== -->

  <!-- SOW Contract extends Gist Agreement -->
  <owl:Class rdf:about="#SemanticSOWContract">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Agreement"/>
    <rdfs:subClassOf rdf:resource="https://agentic-data-scraper.com/ontology/sow#SemanticContract"/>
    <!-- Hierarchical SKOS relationships for taxonomic browsing -->
    <skos:related rdf:resource="#DataBusinessCanvas"/>
    <skos:related rdf:resource="#DataContract"/>
    <rdfs:label>Semantic SOW Contract</rdfs:label>
    <rdfs:comment>Technical implementation contract extending Gist Agreement</rdfs:comment>
    <skos:example>AWS Lambda-based data pipeline contract for Turkish olive oil supplier monitoring with automated web scraping, real-time quality alerts, and S3 data lake integration</skos:example>
    <rdfs:seeAlso rdf:resource="#DataContract"/>
    <rdfs:seeAlso rdf:resource="#DataBusinessCanvas"/>
    <rdfs:seeAlso rdf:resource="https://agentic-data-scraper.com/ontology/complete-sow#BusinessChallenge"/>
