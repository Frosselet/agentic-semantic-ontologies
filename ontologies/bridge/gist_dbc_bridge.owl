    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Project"/>
    <rdfs:label>SOW Project</rdfs:label>
    <rdfs:comment>Project for executing SOW contract deliverables</rdfs:comment>
  </owl:Class>

  <!-- SOW Stakeholders as Gist Persons -->
  <owl:Class rdf:about="#SOWStakeholder">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/semanticarts/ontology/gistCore#Person"/>
    <rdfs:label>SOW Stakeholder</rdfs:label>
    <rdfs:comment>Person involved in SOW contract execution</rdfs:comment>
  </owl:Class>

