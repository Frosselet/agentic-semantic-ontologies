  </owl:ObjectProperty>

  <!-- Key Activities to Value Proposition and Key Resources -->
  <owl:ObjectProperty rdf:about="#enablesDeliveryOf">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:domain rdf:resource="#KeyActivity"/>
    <rdfs:range rdf:resource="#ValueProposition"/>
    <rdfs:label>enables delivery of</rdfs:label>
    <rdfs:comment>Links key activity to value proposition it enables</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#utilizesResource">
    <rdfs:isDefinedBy rdf:resource="https://agentic-data-scraper.com/ontology/gist-dbc-bridge"/>
    <rdfs:domain rdf:resource="#KeyActivity"/>
    <rdfs:range rdf:resource="#KeyResource"/>
    <rdfs:label>utilizes resource</rdfs:label>
